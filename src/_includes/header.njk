        </div>
      </div>
      <div class="-my-2 -mr-2 flex md:hidden">
        {% include 'partials/darkmode.njk' %}
        <button @click="showMobile = true" type="button" class="inline-flex items-center justify-center rounded-md bg-white p-2 text-gray-400 hover:bg-gray-100 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-indigo-500 dark:bg-gray-900" aria-expanded="false">
          <span class="sr-only">Open menu</span>
          <!-- Heroicon name: outline/menu -->
          <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"/>
          </svg>
        </button>
      </div>
      {% if nav.menus %}
        <template x-if="true">
          <nav class="hidden space-x-10 md:flex" x-cloak="x-cloak">
            {% for menu in nav.menus %}
              {% if menu.submenus %}
                <div class="relative" @click.outside="open = false" x-data="{ open: false }">
                  <!-- Item active: "text-gray-900", Item inactive: "text-gray-500" -->
                  <button @click="open = !open" type="button" :class="open ? 'text-gray-900 dark:text-gray-500' : 'text-gray-500 dark:text-gray-400'" class="group inline-flex items-center rounded-md bg-white text-base font-medium hover:text-gray-900 dark:bg-gray-900 dark:hover:text-gray-500" aria-expanded="false">
                    <span>{{ menu.name }}</span>
                    <!-- Heroicon name: solid/chevron-down
                  
                  Item active: "text-gray-600", Item inactive: "text-gray-400" -->
                    <svg class="ml-2 h-5 w-5 text-gray-400 group-hover:text-gray-500" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                      <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd"/>
                    </svg>
                  </button>
                  <!-- Flyout menu, show/hide based on flyout menu state.
                
                Entering: "transition ease-out duration-200"
                    From: "opacity-0 translate-y-1"
                    To: "opacity-100 translate-y-0"
                Leaving: "transition ease-in duration-150"
                    From: "opacity-100 translate-y-0"
                    To: "opacity-0 translate-y-1" -->
                  <div x-show="open" x-transition:enter="transition ease-out duration-200" x-transition:enter-start="opacity-0 translate-y-1" x-transition:enter-end="opacity-100 translate-y-0" x-transition:leave="transition ease-in duration-150" x-transition:leave-start="opacity-100 translate-y-0" x-transition:leave-end="opacity-0 translate-y-1" class="absolute z-50 -ml-4 mt-3 w-screen max-w-md transform px-2 sm:px-0 lg:left-1/2 lg:ml-0 lg:-translate-x-1/2">
                    <div class="overflow-hidden rounded-lg shadow-lg ring-1 ring-black ring-opacity-5 dark:ring-opacity-25">
                      <div class="relative grid gap-6 bg-white px-5 py-6 dark:bg-gray-800 sm:gap-8 sm:p-8">
                        {% for submenu in menu.submenus %}
                          <a href="{{ submenu.route }}" class="-m-3 flex items-start rounded-lg p-3 hover:bg-gray-100 dark:bg-gray-800 dark:hover:bg-gray-700">
                            <!-- Heroicon name: -->
                            <svg class="icon icon-{{ submenu.icon }} flex-shrink-0 h-6 w-6 text-indigo-600" aria-hidden="true" focusable="false" role="presentation">
                              <use href="#icon-{{ submenu.icon }}"/>
                            </svg>
                            <div class="ml-4">
                              <p class="text-base font-medium text-gray-900 dark:text-white">{{ submenu.name }}</p>
                              <p class="mt-1 text-sm text-gray-500 dark:text-gray-400">{{ submenu.description }}</p>
                            </div>
                          </a>
                        {% endfor %}
                      </div>
                      {# {% if not menu.footer == false %}
              <div class="space-y-6 bg-gray-50 px-5 py-5 dark:bg-gray-700 sm:flex sm:justify-around sm:space-y-0 sm:space-x-10 sm:px-8">
