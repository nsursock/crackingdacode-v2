  <div x-cloak="x-cloak" x-transition:enter="duration-200 ease-out" x-transition:enter-start="opacity-0 scale-95" x-transition:enter-end="opacity-100 scale-100" x-transition:leave="duration-100 ease-in" x-transition:leave-start="opacity-100 scale-100" x-transition:leave-end="opacity-0 scale-95" x-show="showMobile" class="absolute inset-x-0 top-0 z-50 origin-top-right transform p-2 transition md:hidden">
